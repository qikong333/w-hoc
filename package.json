{
    "name": "Y-hoc",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build:dev": "npm run lint:code && vite build --mode development",
        "build:pro": " npm run lint:code && vite build --mode production",
        "build": "vite build",
        "serve": "vite preview",
        "lint:code": "eslint --fix src/**/*.{js,ts,tsx,vue}",
        "lint:style": "stylelint --fix src/**/*.{vue,htm,html,css,sss,less,scss}",
        "commit": "git-cz",
        "postinstall": "husky install"
    },
    "dependencies": {
        "@amap/amap-jsapi-loader": "^1.0.1",
        "@amap/amap-jsapi-types": "^0.0.8",
        "@types/geojson": "^7946.0.8",
        "@types/jquery": "^3.5.13",
        "@types/mapbox-gl": "^2.6.3",
        "@types/node": "^18.0.0",
        "@types/qs": "^6.9.7",
        "@zougt/vite-plugin-theme-preprocessor": "^1.4.4",
        "ant-design-vue": "^3.2.7",
        "axios": "^0.24.0",
        "clipboard": "^2.0.8",
        "compression-webpack-plugin": "^9.0.1",
        "dotenv": "^16.0.1",
        "jquery": "^3.6.0",
        "less": "^4.1.2",
        "less-loader": "^10.2.0",
        "mapbox-gl": "^2.7.1",
        "marked": "^4.0.5",
        "mitt": "^3.0.0",
        "mockjs": "^1.1.0",
        "path": "^0.12.7",
        "qs": "^6.10.3",
        "vite-plugin-markdown": "^2.0.2",
        "vite-plugin-pwa": "^0.11.7",
        "vue": "^3.2.16",
        "vue-json-viewer": "3",
        "vue-router": "^4.0.12",
        "vue3-json-viewer": "^1.0.4",
        "vuex": "^4.0.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^15.0.0",
        "@commitlint/config-conventional": "^15.0.0",
        "@types/webpack-env": "^1.16.3",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "@vitejs/plugin-vue": "^1.9.3",
        "@vitejs/plugin-vue-jsx": "^1.3.0",
        "@vue/eslint-config-standard": "^6.1.0",
        "@vue/eslint-config-typescript": "^9.1.0",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.3.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^5.1.1",
        "eslint-plugin-vue": "^8.1.1",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.2",
        "prettier": "^2.5.0",
        "sass": "^1.43.5",
        "stylelint": "^14.1.0",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-standard": "^24.0.0",
        "stylelint-order": "^5.0.0",
        "typescript": "^4.5.2",
        "unplugin-vue-components": "^0.17.2",
        "vite": "^2.6.4",
        "vite-plugin-style-import": "^1.4.0",
        "vite-plugin-windicss": "^1.5.1",
        "windicss": "^3.2.1"
    },
    "lint-staged": {
        "src/**/*.{js,ts,tsx,vue}": [
            "eslint --fix"
        ],
        "src/**/*.{vue,htm,html,css,sss,less,scss}": [
            "stylelint --fix"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "publishConfig": {
        "access": "publish"
    },
    "private": true
}
